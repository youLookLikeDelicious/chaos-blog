{
    "compilerOptions": {
      "lib": ["ES2020", "dom"],
      "target": "ES2019",
      "module": "commonjs",
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
    },
    "exclude": ["node_modules"]
  }